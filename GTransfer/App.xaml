<Application x:Class="GTransfer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GTransfer"
             StartupUri="pack://application:,,,/UserInterfaces/Login.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
              
                <ResourceDictionary Source="pack://application:,,,/Styles/Styles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Styles/ValueConverters.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
