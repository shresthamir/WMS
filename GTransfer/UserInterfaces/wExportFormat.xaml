<Window x:Class="GTransfer.UserInterfaces.wExportFormat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="wExportFormat" Height="130" Width="300">
    <Border Style="{StaticResource PlainBackground}">
        <StackPanel>
            <StackPanel Orientation="Horizontal" Margin="5 10 5 5 ">
                <Label Content="Export Format" Width="90"/>
                <ComboBox Width="170" x:Name="cmbFormat">
                    <ComboBox.Items>
                        <ComboBoxItem>Export To Excel</ComboBoxItem>
                        <ComboBoxItem>Export To PDF</ComboBoxItem>
                        <ComboBoxItem>Export To XML</ComboBoxItem>
                        <ComboBoxItem>Export To CSV</ComboBoxItem>
                    </ComboBox.Items>
                </ComboBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <Button Style="{StaticResource NewButton}" Height="30" Width="60" Content="Ok" Click="Button_Click"/>
                <Button Style="{StaticResource NewButton}" Height="30" Width="60" Content="Cancel" Click="Button_Click"/>
            </StackPanel>
        </StackPanel>
    </Border>

</Window>

    